{
	"cSpell.ignorePaths": [
		"package-lock.json",
		"node_modules",
		"vscode-extension",
		".git/objects",
		".vscode",
		".vscode-insiders",
		"**/test/**/*"
	],
	"cSpell.words": [
		"algorythmic",
		"cargile",
		"fkt",
		"geohash",
		"grf",
		"grouprunfinder",
		"IOei",
		"openendurance",
		"openenduranceinitiative",
		"speedwork",
		"strava",
		"ultrarunning"
	],
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"files.exclude": {
		"**/.git": true,
		"**/node_modules": true,
		"**/.DS_Store": true,
		".{dev,shadowenv.d}": true
	},
	"javascript.validate.enable": false,
	"typescript.tsdk": "./node_modules/typescript/lib"
}
